# Example environment variables for Wazuh
# Copy to .env and replace placeholder values.
OSSEC_LOGALL=yes
OSSEC_LOGCOLLECTOR=yes
OSSEC_LOGCOLLECTOR_INTERVAL=10
OSSEC_LOGCOLLECTOR_BUFFER=1000
OSSEC_LOGCOLLECTOR_MAXSIZE=1048576
OSSEC_OUTPUT_TYPE=elasticsearch
OSSEC_OUTPUT_HOST=elasticsearch
OSSEC_OUTPUT_PORT=9200
OSSEC_OUTPUT_INDEX=wazuh
OSSEC_OUTPUT_USER=elastic
OSSEC_OUTPUT_PASSWORD=changeme
OSSEC_ALERTS_EMAIL_NOTIFICATION=no
